{application, sflog, 
 [{description, "SimpleFileLOGger for Erlang"},
  {vsn, "1.0"},
  {modules, [sflog, sflog_server, sflog_sup, sflog_app]},	
  {registered,[sflog]},
  {applications, [kernel,stdlib]},
  {mod, {sflog_app,[]}},
  {start_phases, []},
  {env, [   
	{logfile, "sflog"},
	{logdir, "/tmp"}
  ]}
 ]}.
